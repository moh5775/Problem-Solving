#include<stdio.h>
int main()
{
    int y,w;
    scanf("%d %d",&y,&w);
    //possible outcome calculation
    int k=6-(y>w?y:w)+1;
    if(k==1)
        printf("1/6\n");
    else if(k==2)
        printf("1/3\n");
    else if(k==3)
        printf("1/2\n");
    else if(k==4)
        printf("2/3\n");
    else if(k==5)
        printf("5/6\n");
    else if(k==6)
        printf("1/1\n");

}