#include<stdio.h>
int main ()
{
    int t;
    scanf("%d",&t);
    for(int j=1; j<=t; j++)
    {
        int n,i;
        scanf("%d",&n);
        int count = 0;
        int dig;
        for(i=1; count<n; i++)
        {
            if(i%3!=0 && i%10!=3)
            {
                dig=i;
                count++;
            }
            else
                continue;
        }
        printf("%d\n",dig);
    }
}