#include<stdio.h>
int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int a,b,c;
        scanf("%d %d %d",&a,&b,&c);
        for(int i=1; i<=5; i++)
        {
            if(a<=b&&a<=c)
                a=a+1;
            else if(b<=c&&b<=a)
                b=b+1;
            else
                c=c+1;

        }
        printf("%d\n",a*b*c);
    }
}
