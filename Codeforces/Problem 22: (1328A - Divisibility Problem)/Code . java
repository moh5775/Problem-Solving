#include<stdio.h>
int main()
{
    long long m,n,i,t,j;
    scanf("%lld",&t);
    long long count;
    for(i=1; i<=t; i++)
    {

        int rem,inc;
        scanf("%lld %lld",&m,&n);
        {

            rem=m%n;
            if(rem>0)
                inc=n-rem;
            else
                inc=0;
        }
        printf("%d\n",inc);

    }

}
