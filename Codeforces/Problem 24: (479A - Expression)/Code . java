#include<stdio.h>
int main()
{
    int x,y,z,m,n,p,r,s,t,u;
    scanf("%d %d %d",&x,&y,&z);
    m=x+(y*z);
    n=x*(y+z);
    p=x*y*z;
    r=(x+y)*z;
    t=(x*y)+z;
    u=x+y+z;
    int max=m;
    if(n>max)
        max=n;
    if(p>max)
        max=p;
    if(r>max)
        max=r;
    if(t>max)
        max=t;
    if(u>max)
        max=u;
    printf("%d\n",max);
}
