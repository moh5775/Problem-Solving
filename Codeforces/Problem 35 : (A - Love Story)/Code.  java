
import java.util.Scanner;

public class ewe {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int t = input.nextInt();
        input.nextLine();

        for (int i = 1; i <= t; i++) {
            String s = "codeforces";
            String str = input.nextLine();
            int count = 0;
            for (int j = 0; j < s.length(); j++) {
                if (str.charAt(j) != s.charAt(j))
                    count++;
            }

            System.out.println(count);
        }
    }
}
